@model Actor

 <h1>----------------------------------</h1>
    <h2>@Model.Name</h2>
    <h4>Salary: $@Model.Salary</h4>
    <h4>Roles:</h4>
    @if (Model.AllRoles.Count > 0)
    {
        foreach (var kvp in Model.AllRoles)
        {
            <h5>@kvp.Key.Name as @kvp.Value</h5>
        }
    }
    else
    {
            <h5>This movie has no Actors</h5>
    }
    <h4>Ratings:</h4>
    @if (Model.AllRatings.Count > 0)
    {
        foreach (Rating rating in Model.AllRatings)
        {
            <h5>@rating.User.Name: @rating.Score</h5>
        }
    }
    else
    {
        <h5>This Actor has no ratings</h5>
    }
    <h1>----------------------------------</h1>